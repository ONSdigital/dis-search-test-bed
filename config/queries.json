[
  {
    "query": "golang best practices",
    "algorithm": "baseline",
    "description": "Basic multi-match on title and body",
    "es_query": {
      "query": {
        "multi_match": {
          "query": "golang best practices",
          "fields": ["title^2", "body"]
        }
      }
