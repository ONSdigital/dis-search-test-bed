  },
  {
    "query": "golang best practices",
    "algorithm": "freshness_boosted",
    "description": "Boost recent articles, penalize old ones",
    "es_query": {
      "query": {
        "function_score": {
          "query": {
            "multi_match": {
              "query": "golang best practices",
              "fields": ["title^2", "body"]
            }
          },
          "functions": [
            {
              "gauss": {
                "date": {
                  "origin": "now",
                  "scale": "90d",
                  "decay": 0.5
                }
              },
              "weight": 2
            }
          ],
          "boost_mode": "multiply"
        }
      }
    }
  }
