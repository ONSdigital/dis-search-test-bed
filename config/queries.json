  {
    "name": "recency_boosted",
    "description": "Custom scoring with date-based decay for freshness",
    "queries": [
      {
        "query": "golang concurrency best practices",
        "description": "Recent content gets higher scores",
        "es_query": {
          "query": {
            "function_score": {
              "query": {
                "multi_match": {
                  "query": "golang concurrency best practices",
                  "fields": ["title^2", "body"],
                  "type": "best_fields"
                }
              },
              "functions": [
                {
                  "gauss": {
                    "date": {
                      "origin": "now",
                      "scale": "365d",
                      "offset": "10d",
                      "decay": 0.33
                    }
                  },
                  "weight": 2
                }
              ],
              "boost_mode": "sum",
              "score_mode": "sum",
              "max_boost": 42
            }
          },
          "size": 20,
          "min_score": 0.1
        }
      },
      {
        "query": "testing",
        "description": "Single term with recency bias",
        "es_query": {
          "query": {
            "function_score": {
              "query": {
                "match": {
                  "body": "testing"
                }
              },
              "functions": [
                {
                  "gauss": {
                    "date": {
                      "origin": "now",
                      "scale": "365d",
                      "offset": "10d",
                      "decay": 0.33
                    }
                  },
                  "weight": 1.5
                }
              ],
              "boost_mode": "sum",
              "score_mode": "sum"
            }
          },
          "size": 20
        }
      },
      {
        "query": "best practices",
        "description": "Common phrase with decay function",
        "es_query": {
          "query": {
            "function_score": {
              "query": {
                "match_phrase": {
                  "body": "best practices"
                }
              },
              "functions": [
                {
                  "gauss": {
                    "date": {
                      "origin": "now",
                      "scale": "90d",
                      "offset": "30d",
                      "decay": 0.5
                    }
                  },
                  "weight": 1.8
                }
              ],
              "boost_mode": "multiply",
              "score_mode": "sum"
            }
          },
          "size": 20
        }
      },
      {
        "query": "microservices architecture design",
        "description": "Multi-concept query with aggressive recency boost",
        "es_query": {
          "query": {
            "function_score": {
              "query": {
                "bool": {
                  "must": [
                    {
                      "multi_match": {
                        "query": "microservices architecture design",
                        "fields": ["title^2", "body"]
                      }
                    }
                  ]
                }
              },
              "functions": [
                {
                  "gauss": {
                    "date": {
                      "origin": "now",
                      "scale": "180d",
                      "offset": "15d",
                      "decay": 0.25
                    }
                  },
                  "weight": 2.5
                }
              ],
              "boost_mode": "sum",
              "score_mode": "sum",
              "max_boost": 50
            }
          },
          "size": 20
        }
      }
    ]
  },
