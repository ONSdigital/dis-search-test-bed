[
  {
    "name": "bm25_default",
    "description": "Standard BM25 algorithm with default parameters",
    "queries": [
      {
        "query": "golang concurrency best practices",
        "description": "Multi-word query testing relevance ranking",
        "es_query": {
          "query": {
            "multi_match": {
              "query": "golang concurrency best practices",
              "fields": ["title^3", "body^1"],
              "type": "best_fields",
              "operator": "or"
            }
          },
          "size": 20
        }
      },
      {
        "query": "testing",
        "description": "Single word query with high recall",
        "es_query": {
          "query": {
            "match": {
              "body": {
                "query": "testing",
                "fuzziness": "AUTO"
              }
            }
          },
          "size": 20
        }
      },
      {
        "query": "elasticsearch integration tutorial",
        "description": "Phrase-like query combining multiple concepts",
        "es_query": {
          "query": {
            "bool": {
              "should": [
                {
                  "match": {
                    "title": {
                      "query": "elasticsearch integration tutorial",
                      "boost": 2
                    }
                  }
                },
                {
                  "match": {
                    "body": {
                      "query": "elasticsearch integration tutorial"
                    }
                  }
                }
              ]
            }
          },
          "size": 20
        }
      },
      {
        "query": "performance optimization",
        "description": "Query testing content type filtering",
        "es_query": {
          "query": {
            "bool": {
              "must": [
                {
                  "multi_match": {
                    "query": "performance optimization",
                    "fields": ["title", "body"]
                  }
                }
              ],
              "filter": [
                {
                  "term": {
                    "content_type": "article"
                  }
                }
              ]
            }
          },
          "size": 20
        }
      }
    ]
  },
